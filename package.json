{
  "name": "lecto-gym",
  "homepage": "https://virtualmonkey.github.io/lecto-gym",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "add": "^2.0.6",
    "axios": "^1.3.3",
    "lodash": "^4.17.21",
    "normalizr": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "5.3.4",
    "react-scripts": "5.0.1",
    "react-timer-hook": "^3.0.5",
    "reactjs-popup": "^2.0.5",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.2",
    "sass": "^1.58.3",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.0",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "jest-enzyme": "^7.1.2",
    "jest-react-hooks-shallow": "^1.5.1",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/components/Nav/",
      "<rootDir>/src/components/Timer/",
      "<rootDir>/src/pages/",
      "<rootDir>/src/router/",
      "<rootDir>/src/App.js",
      "<rootDir>/src/index.js",
      "<rootDir>/src/reportWebVitals.js",
      "<rootDir>/src/redux/rootReducer.js",
      "<rootDir>/src/redux/rootSaga.js",
      "<rootDir>/src/redux/store.js",
      "<rootDir>/src/redux/weeks/weeks.schemas.js",
      "<rootDir>/src/redux/week/week.schemas.js"
    ]
  }
}
